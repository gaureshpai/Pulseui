{
  "name": "@pulseui/icons",
  "version": "1.0.0",
  "description": "Comprehensive icon package for Pulse UI design system with MUI icons and design tokens",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.esm.js",
      "require": "./dist/icons.js",
      "types": "./dist/icons.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@mui/icons-material": "^5.0.0"
  },
  "dependencies": {
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "rollup": "^4.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "icons",
    "mui",
    "material-ui",
    "design-system",
    "react",
    "typescript"
  ],
  "author": "Pulse UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pulseui/icons.git"
  },
  "bugs": {
    "url": "https://github.com/pulseui/icons/issues"
  },
  "homepage": "https://github.com/pulseui/icons#readme"
}
