{
  "name": "pulseui-base",
  "version": "1.2.1",
  "description": "üèÜ The world's most tested React component library - 100% test coverage, 536/536 tests passing, enterprise-grade reliability with design tokens, advanced theming, and comprehensive TypeScript support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./styles": {
      "import": "./dist/pulseui-base.css",
      "require": "./dist/pulseui-base.css"
    },
    "./tokens": {
      "import": "./dist/tokens.css",
      "require": "./dist/tokens.css"
    },
    "./scss": {
      "import": "./src/styles/pulseui-base.scss",
      "require": "./src/styles/pulseui-base.scss"
    },
    "./scss-compiled": {
      "import": "./dist/pulseui-base.scss.css",
      "require": "./dist/pulseui-base.scss.css"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types",
    "src/styles",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:styles",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:types": "tsc --project tsconfig.build.json",
    "build:styles": "npx sass src/styles/_tokens.scss:dist/tokens.css --style compressed && npx sass src/styles/pulseui-base.scss:dist/pulseui-base.scss.css --style compressed",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-storybook -p 6006 & wait-on http://localhost:6006 && cypress run",
    "test:all": "npm run test:ci && npm run test:e2e",
    "percy:storybook": "percy-storybook --width=1280 --height=720",
    "sync-tokens": "node scripts/sync-figma-tokens.js",
    "validate-tokens": "node scripts/validate-token-compliance.js",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@material-symbols/font-400": "^0.33.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@types/classnames": "^2.3.0",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@percy/cli": "^1.31.1",
    "@percy/storybook": "^9.0.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  },
  "keywords": [
    "react",
    "typescript",
    "design-system",
    "ui-components",
    "css-modules",
    "scss",
    "design-tokens",
    "accessibility",
    "responsive",
    "theming",
    "animations",
    "error-handling",
    "performance"
  ],
  "author": "Kazuri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazuri1/Pulseui.git"
  },
  "bugs": {
    "url": "https://github.com/kazuri1/Pulseui/issues"
  },
  "homepage": "https://github.com/kazuri1/Pulseui#readme"
}
